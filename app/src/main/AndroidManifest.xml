        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name=".Activity.Splash_Screen"
            android:screenOrientation="portrait"
            >


            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Story_Activity"/>
        <activity android:name=".Activity.Payment_Activity"/>
        <activity
            android:name=".MainLayout"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".Activity.Recycler_Activity"/>
        <activity android:name=".Activity.Shopkeeper_Activity"/>
        <activity android:name=".HostelName.RT_Hall"/>
        <activity android:name=".HostelName.MV_hall"/>
        <activity android:name=".HostelName.SJ_hall"/>
        <activity android:name=".HostelName.Meghamani_Parivar"/>
        <activity android:name=".HostelName.Diamond_jubile"/>
        <activity android:name=".HostelName.Others_Address"/>
